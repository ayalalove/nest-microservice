generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id          Int      @id @default(autoincrement())
  name        String
  create_date DateTime @default(now())
  latitude    Float
  longitude   Float
  alerts      Int[]
  status      Int
  description String?
  updatedAt   DateTime @updatedAt
}